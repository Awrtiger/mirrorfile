{
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "style": "plain",
                    "properties": {
                        "macos": "mac"
                    },
                    "foreground": "#26C6DA"
                },
                {
                    "type": "session",
                    "style": "plain",
                    "foreground": "#3A86FF"
                },
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#8800dd",
                    "properties": {
                        "always_enabled": true,
                        "style": "austin"
                    }
                },
                {
                    "type": "path",
                    "style": "plain",
                    "foreground": "#ff479c",
                    "properties": {
                        "style": "full",
                        "prefix": " \uf07b ",
                        "folder_separator_icon": "/"
                    }
                },
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#4caf50",
                    "properties": {
                        "time_format": "2006-01-02 15:04:05"
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#14A5AE",
                    "properties": {
                        "display_stash_count": true,
                        "display_upstream_icon": true,
                        "prefix": ""
                    }
                },
                {
                    "type": "node",
                    "style": "plain",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#6CA35E",
                    "properties": {
                        "prefix": " \uE718 "
                    }
                },
                {
                    "type": "root",
                    "style": "plain",
                    "foreground": "#ffff66"
                },
                {
                    "type": "exit",
                    "style": "plain",
                    "foreground": "#2e9599",
                    "properties": {
                        "display_exit_code": true,
                        "always_enabled": true,
                        "error_color": "#f1184c",
                        "prefix": " \ue3bf "
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "shell",
                    "style": "diamond",
                    "foreground": "#0077c2",
                    "properties": {
                        "mapped_shell_names": {
                            "pwsh": "PS"
                        },
                        "prefix": " \uf489 ",
                        "postfix": " \uE0B1 "
                    }
                }
            ]
        }
    ]
}